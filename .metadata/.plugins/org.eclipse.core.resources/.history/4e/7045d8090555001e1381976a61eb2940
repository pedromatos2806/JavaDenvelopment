import entities.PessoaFisica;
import entities.PessoaJuridica;
import exceptions.RendaBrutaNegativeException;


public class Teste {

	public static void main(String[] args) throws RendaBrutaNegativeException {
	PessoaFisica pessoa1 = new PessoaFisica(1400);
	PessoaFisica pessoa2 = new PessoaFisica(1401);
	PessoaFisica pessoa3 = new PessoaFisica(2101);
	
	PessoaJuridica pessoa4 = new PessoaJuridica(1400);
	PessoaJuridica pessoa5 = new PessoaJuridica(1401);
	PessoaJuridica pessoa6 = new PessoaJuridica(2101);
	
	pessoa1.toString();
	pessoa2.toString();
	pessoa3.toString();
	
	pessoa4.toString();
	pessoa5.toString();
	pessoa6.toString();
	}

}
