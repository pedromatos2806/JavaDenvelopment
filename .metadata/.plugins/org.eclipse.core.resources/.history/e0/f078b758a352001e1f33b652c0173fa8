import java.util.*;


public class Testes {


	public static void main(String[] args) {
		ContaBancaria conta= new ContaBancaria("Pedro Matos", "1234567890",1000);
		System.out.println("Nome: " + conta.getNomeCliente());
		System.out.println("Numero da conta: " + conta.getNumConta());
		System.out.println("Saldo " + conta.getSaldo());
		
		try {
			conta.Sacar(900);
		}catch (Exception e){
			System.out.println("Saldo insuficiente para saque!");
		}
		System.out.println("Novo saldo: "+ conta.getSaldo());
		
		ContaEspecial contaEspecial= new ContaEspecial("Pedro Especial","91827273ESP",1000,10);
	
		System.out.println("Nome: " + contaEspecial.getNomeCliente());
		System.out.println("Numero da conta: " + contaEspecial.getNumConta());
		System.out.println("Saldo " + contaEspecial.getSaldo());
		
		try {
			contaEspecial.Sacar(900);
		}catch (Exception e){
			System.out.println("Saldo insuficiente para saque!");
		}
		System.out.println("Novo saldo: "+ contaEspecial.getSaldo());
		
	}
}
