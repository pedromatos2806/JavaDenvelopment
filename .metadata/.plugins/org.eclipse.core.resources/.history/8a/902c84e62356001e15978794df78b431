
public class Peixe extends Animal{
	
	private double barbatanas;
	
	private String cauda;
	
	public Peixe (String animal, double comprimento , String cor , double velocidade, double barbatanas, String cauda) {
		super(animal,comprimento ,cor ,velocidade);
		this.patas = 0;
		this.ambiente = "mar";
	}
	
	public Peixe (String animal, double comprimento , String cor , String ambiente , double velocidade) {
		super(animal,comprimento ,cor ,ambiente, velocidade);
		this.patas = 0;
	}
	
	public double getBarbatanas() {
		return this.barbatanas;
	}
	
	public void setBarbatanas(String barbatanas) {
		this.barbatanas = barbatanas;
	}
	
	public String cauda() {
		return this.cauda;
	}
}
