package Entities;

public class Mamifero extends Animal{
	private String alimento;
	
	public Mamifero() {
		super();
	}
	
	public Mamifero(String animal,Double patas, double comprimento, String cor, double velocidade,String alimento) {
		super(animal,comprimento,patas,velocidade);
		// TODO Auto-generated constructor stub
		this.ambiente = "Terra";
		this.alimento = alimento;
	}
	
	public String getAlimento() {
		return this.alimento;
	}
	
	public void setAlimento(String alimento) {
		this.alimento = alimento;
	}
	
	@Override
	public String toString() {
		super.toString();
		return "\nAlimento: " + alimento;
	}
}